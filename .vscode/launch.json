{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "kite调试-server",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceRoot}/GO/kite/main.go",
			"args": [
				"--func=server",
				"--path=E:\\git\\learn\\GO\\kite",
				"--workspace=/1/2"
			]
		},
		{
			"name": "kite调试-client",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceRoot}/GO/kite/main.go",
			"args": [
				"--func=client",
				"--path=E:\\git\\learn\\GO\\kite",
				"--cmd=list"
			]
		},
		{
			"name": "远程调试",
			"type": "go",
			"request": "launch",
			"mode": "remote",
			"remotePath": "${workspaceRoot}",
			"port": 2345,
            "host": "10.125.42.41",
			"program": "${workspaceRoot}/GO/src/main.go",
			"args": [
				"--func=client",
				"--path=E:\\git\\learn\\GO\\kite",
				"--cmd=update"
			],
			"showLog": true
		},
		{
			"name": "Launch index.html with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"file": "${workspaceRoot}/index.html",
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8080",
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "Attach with sourcemaps",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		}
	]
}