{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "kite调试-server",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceRoot}/GO/kite/main.go",
			"args": [
				"--func=server",
				"--path=E:\\git\\learn\\GO\\kite",
				"--workspace=D:\\log"
			]
		},
		{
			"name": "kite调试-client",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceRoot}/GO/kite/main.go",
			"args": [
				"--func=client",
				"--path=E:\\git\\learn\\GO\\kite",
				"--cmd=update",
				"--b=test1"
			]
		},
		{
			"name": "hello",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceRoot}/GO/src/test/hello.go",
			"args": [
			],
			"showLog": false
		},
		{
			"name": "ch8",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceRoot}/GO/src/ch8/main.go",
			"args": [
				"--func=ch8.Spinner",
				"--args=args"
			],
			"showLog": false
		},
		{
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8080",
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "Attach with sourcemaps",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		}
	]
}