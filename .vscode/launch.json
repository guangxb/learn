{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "deployment调试",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceRoot}/GO/deployment/main.go",
			"args": [
				"--func=config-list",
				"--args=a"
			]
		},
		{
			"name": "hello调试",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceRoot}/GO/src/main.go",
			"args": [
				"--func=crawler2"
			]
		},
		{
			"name": "Launch index.html with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"file": "${workspaceRoot}/index.html",
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8080",
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "Attach with sourcemaps",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}"
		}
	]
}